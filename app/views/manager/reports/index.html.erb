<%= provide :title, t(".title") %>
<div class="div-all-report">
  <h2 class="title-center report-title">
    <%= t ".title" %>
  </h2>
  <div class="col-xs-12">
    <div class="div-report-filter">
      <%= render "filter_form" %>
    </div>
    <div id="all-report">
      <%= render "all_reports", reports: @reports, page: @pagy %>
    </div>
  </div>
</div>
