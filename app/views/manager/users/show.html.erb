<%= provide :title, @user.name %>
<div class="col-md-12 box-show box-account-info">
  <%= render "shared/account_info", user: @user %>
  <%= button_to t(".remove"), manager_user_path(@user), method: :delete, data: { confirm: t(".confirm_delete") }, class: "btn btn-danger danger",remote: :true %>
</div>
